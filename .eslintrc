{
  "ecmaFeatures": {
    "modules": true
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "quotes": [1, "single"],
    "comma-dangle": true,
		"no-use-before-define": false
    "no-undef": 2,
    "no-unused-vars": 2
  }
}
